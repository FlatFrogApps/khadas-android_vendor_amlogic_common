bootstrap_go_package {
    name: "soong-droidlogic_icu",
    pkgPath: "android/soong/vendor/amlogic/common/prebuilt/icu",
    deps: [
        "blueprint",
        "blueprint-pathtools",
        "soong",
        "soong-android",
        "soong-cc",
        "soong-genrule",
    ],
    srcs: [
        "droidlogic_icu.go",
    ],
    pluginFor: ["soong_build"],
}


droidlogic_icu_go_defaults {
    name: "droidlogic_icu_go_defaults",
}

cc_prebuilt_library_static {

    name: "libicuuc_vendor_static",
    compile_multilib: "32",
    vendor: true,
    defaults: ["droidlogic_icu_go_defaults"],
    arch: {
        arm: {
            srcs: ["lib/libicuuc_vendor.a"],
        },
        arm64: {
            srcs: ["lib64/libicuuc_vendor.a"],
        },
    },

    export_include_dirs: ["include"],

    shared_libs: [
        "libcrypto",
        "libcutils",
        "liblog",
        "libutils",
        "libz",
    ],

}

cc_prebuilt_library_shared {

    name: "libicuuc_vendor",
    compile_multilib: "32",
    vendor: true,
    defaults: ["droidlogic_icu_go_defaults"],
    arch: {
        arm: {
            srcs: ["lib/libicuuc_vendor.so"],
        },
        arm64: {
            srcs: ["lib64/libicuuc_vendor.so"],
        },
    },

    export_include_dirs: ["include"],

    shared_libs: [
        "libcrypto",
        "libcutils",
        "liblog",
        "libutils",
        "libz",
    ],

}

cc_prebuilt_library_shared {

    name: "libicui18n_vendor",
    export_include_dirs: ["include"],
    vendor: true,
    defaults: ["droidlogic_icu_go_defaults"],

    compile_multilib: "32",
    arch: {
        arm: {
            srcs: ["lib/libicui18n_vendor.so"],
        },
        arm64: {
            enabled: false,
            //srcs: ["lib64/libicui18n_vendor.so"],
        },
        x86: {
            enabled: false,
        },
        x86_64: {
            enabled: false,
        },
    },
    shared_libs: [
        "libcrypto",
        "libcutils",
        "libicuuc_vendor",
        "liblog",
        "libutils",
        "libz",
    ],

}

